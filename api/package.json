{
  "name": "pm-tools-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "type": "commonjs",
  "license": "UNLICENSED",
  "engines": {
    "node": "22.14.0",
    "pnpm": ">=10.7.0"
  },
  "scripts": {
    "build": "nest build",
    "start": "nest start -b swc",
    "start:dev": "nest start -b swc -w",
    "start:debug": "nest start -b swc --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:create": "mikro-orm migration:create --config ./src/configs/mikro-orm.config.ts",
    "migration:up": "mikro-orm migration:up --config ./src/configs/mikro-orm.config.ts",
    "migration:down": "mikro-orm migration:down --config ./src/configs/mikro-orm.config.ts",
    "migration:pending": "mikro-orm migration:pending --config ./src/configs/mikro-orm.config.ts",
    "migration:fresh": "mikro-orm migration:fresh --config ./src/configs/mikro-orm.config.ts",
    "seed:create": "mikro-orm seeder:create --config ./src/configs/mikro-orm.config.ts",
    "seed:run": "mikro-orm seeder:run --config ./src/configs/mikro-orm.config.ts",
    "db:refresh": "mikro-orm migration:fresh --seed --config ./src/configs/mikro-orm.config.ts"
  },
